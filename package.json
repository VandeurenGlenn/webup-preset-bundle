{
  "name": "webup-preset-bundle",
  "version": "1.1.0",
  "main": "dist/bundle-node.js",
  "module": "dist/bundle-es.js",
  "keywords": [
    "webup",
    "preset",
    "bundle"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vandeurenglenn/webup-preset-bundle.git"
  },
  "author": "Glenn Vandeuren <vandeurenglenn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vandeurenglenn/webup-preset-bundle/issues"
  },
  "homepage": "https://github.com/vandeurenglenn/webup-preset-bundle#readme",
  "scripts": {
    "build": "backed -b",
    "test": "mocha"
  },
  "description": "",
  "dependencies": {
    "polymer-bundler": "^2.0.3"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "backed-cli": "^0.6.0",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "rollup-plugin-babel": "^2.7.1"
  },
  "backed": {
    "bundles": [
      {
        "src": "src/bundle.js",
        "dest": "dist/bundle.js",
        "format": "cjs",
        "plugins": {
          "babel": {
            "presets": [
              [
                "es2015",
                {
                  "modules": false
                }
              ]
            ],
            "plugins": [
              "external-helpers"
            ]
          }
        }
      },
      {
        "src": "src/bundle.js",
        "dest": "dist/bundle.js",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ],
            "plugins": [
              "external-helpers"
            ]
          }
        }
      }
    ]
  }
}
